{
  "name": "vuex-cli-webpack",
  "version": "2.0.0-beta3",
  "description": "vuex-cli 中的 webpack 任务。",
  "main": "index.js",
  "engines": {
    "node": ">=6.2.0",
    "npm": "^3.0.0"
  },
  "bin": {
    "vuex-webpack-server": "bin/vuex-webpack-server.js",
    "vuex-webpack-compile": "bin/vuex-webpack-compile.js"
  },
  "scripts": {
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src ./node_modules/.bin/babel-node ./node_modules/.bin/_mocha --compilers js:babel-register --opts ./.mocharc ",
    "start": "npm run build:watch",
    "build": "./node_modules/.bin/babel src -d lib",
    "build:watch": "./node_modules/.bin/babel src --watch -d lib",
    "clean": "rimraf lib",
    "publish:patch": "npm test && npm run clean && npm run build && npm version patch && npm publish",
    "publish:minor": "npm test && npm run clean && npm run build && npm version minor && npm publish",
    "release": "bash .tools/build/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sokis/vuex-cli-webpack.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "webpack",
    "vuex-cli",
    "vuex-webpack"
  ],
  "author": "sokis <zengyi.zal@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "babel-types": "^6.18.0",
    "chai": "^3.5.0",
    "compression": "^1.6.2",
    "css-loader": "^0.23.1",
    "cssnano": "^3.8.0",
    "debug": "^2.3.2",
    "eslint": "^3.10.1",
    "eslint-config-vue": "^2.0.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^3.3.2",
    "eslint-plugin-vue": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.30.0",
    "history": "^2.1.2",
