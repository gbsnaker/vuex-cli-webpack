{
  "name": "vuex-cli-webpack",
  "version": "1.0.6",
  "description": "vuex-cli 中的 webpack 任务。",
  "main": "index.js",
  "engines": {
    "node": ">=6.2.0",
    "npm": "^3.0.0"
  },
  "bin": {
    "vuex-webpack-server": "bin/vuex-webpack-server.js",
    "vuex-webpack-compile": "bin/vuex-webpack-compile.js"
  },
  "scripts": {
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src ./node_modules/.bin/babel-node ./node_modules/.bin/_mocha --compilers js:babel-register --opts ./.mocharc ",
    "start": "npm run build:watch",
    "build": "./node_modules/.bin/babel src -d lib",
    "build:watch": "./node_modules/.bin/babel src --watch -d lib",
    "clean": "rimraf lib",
    "publish:patch": "npm test && npm run clean && npm run build && npm version patch && npm publish",
    "publish:minor": "npm test && npm run clean && npm run build && npm version minor && npm publish",
    "release": "bash .tools/build/release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sokis/vuex-cli-webpack.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "webpack",
    "vuex-cli",
    "vuex-webpack"
  ],
  "author": "sokis <zengyi.zal@gmail.com>",
  "license": "MIT",
